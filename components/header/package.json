{
  "name": "header",
  "description": "",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx src",
    "build": "tsc --noEmit && oc package .",
    "test": "vitest",
    "start": "oc dev .. --components header"
  },
  "oc": {
    "files": {
      "data": "src/server.ts",
      "template": {
        "src": "src/App.tsx",
        "type": "oc-template-react"
      },
      "static": [
        "public"
      ]
    },
    "parameters": {
      "userId": {
        "default": 1,
        "description": "The user id from the user database",
        "example": 1,
        "mandatory": true,
        "type": "number"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "jsdom": "^21.1.1",
    "oc-server": "^0.4.2",
    "oc-template-react-compiler": "6.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.3",
    "vitest": "^0.32.2"
  }
}
